    "@neondatabase/serverless": "^0.9.4",
    "arctic": "^1.9.2",
    "drizzle-orm": "^0.32.2",
    "elysia": "1.1.2",
    "elysia-oauth2": "^1.3.0",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@types/shelljs": "^0.8.15",
