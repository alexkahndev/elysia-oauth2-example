{
  "name": "awesome-social",
  "version": "0.2.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css}\"",
    "dev": "bun run --watch src/server.ts",
